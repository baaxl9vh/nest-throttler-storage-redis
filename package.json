{
  "name": "nest-throttler-storage-redis",
  "version": "0.0.1",
  "description": "nestjs throttler storage redis service base on node-redis",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "test": "jest --config ./tests/jest-e2e.json --runInBand",
    "format": "prettier --write \"{lib,src,test}/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "postversion": "git push --follow-tags",
    "release:dry": "release-it --dry-run",
    "release": "release-it",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baaxl9vh/nest-throttler-storage-redis.git"
  },
  "keywords": [
    "nest",
    "nestjs",
    "throttler",
    "storage",
    "redis"
  ],
  "author": "jay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baaxl9vh/nest-throttler-storage-redis/issues"
  },
  "homepage": "https://github.com/baaxl9vh/nest-throttler-storage-redis#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/common": "^8.2.4",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/testing": "^8.2.4",
    "@nestjs/throttler": "^2.0.0",
    "@release-it/conventional-changelog": "^4.1.0",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "redis": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^14.12.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
